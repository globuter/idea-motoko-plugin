let bitwiseAnd = 1 & 1;
let bitwiseOr = 1 | 1;
let exclusiveOr = 1 ^ 1;
let shiftLeft = 1 << 1;
let shiftRight = 1 >> 1;
let rotateLeft = 1 <<> 1;
let rotateRight = 1 <>> 1;

let wrappingAddition = 1 +% 1;
let wrappingSubtraction = 1 -% 1;
let wrappingMultiplication = 1 *% 1;
let wrappingExponentiation = 1 **% 1;
